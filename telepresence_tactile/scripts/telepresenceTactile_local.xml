<application>
	<name>Telepresence_Tactile_Local</name>
	<module>
		<name>telepresenceTactile</name>
		<parameters></parameters>
        <node>localhost</node>
        <stdio></stdio>
	</module>

	<connection>
		<from>/icub/skin/left_hand_comp</from>
		<to>/telepresence/tactile/left:i</to>
		<protocol>tcp</protocol>
 	</connection>

	<connection>
		<from>/icub/skin/right_hand_comp</from>
		<to>/telepresence/tactile/right:i</to>
		<protocol>tcp</protocol>
 	</connection>
</application>
